{
    "name": "permittedleader/forms-for-laravel",
    "description": "Forms for Laravel",
    "type": "project",
    "require": {
        "livewire/livewire": ">=3.0",
        "laravel/framework": ">=10.0",
        "propaganistas/laravel-phone": "^5.1",
        "permittedleader/tiffey": ">=1.0",
        "asdh/laravel-flatpickr": "^2.2"
    },
    "repositories": {
        "tiffey": {
            "type": "vcs",
            "url": "https://github.com/PermittedLeader/tiffey.git"
        }
    },
    "license": "GNU GPL v3",
    "autoload": {
        "psr-4": {
            "Permittedleader\\Forms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Permittedleader\\Forms\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Myles Smith",
            "email": "159888845+PermittedLeader@users.noreply.github.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Permittedleader\\Forms\\FormsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-livewire-v3": "2.0.x-dev"
        }
    }
}
